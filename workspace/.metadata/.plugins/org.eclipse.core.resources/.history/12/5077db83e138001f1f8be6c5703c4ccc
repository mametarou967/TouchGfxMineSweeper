#include <gui/model/Model.hpp>
#include <gui/model/ModelListener.hpp>

Model::Model() : modelListener(0)
{
    for (int row = 0; row < size; ++row) {
        for (int column = 0; column < size; ++column) {
            matrix[i][j] = 0;  // 全ての要素を0に初期化
        }
    }
}

void Model::tick()
{

}
