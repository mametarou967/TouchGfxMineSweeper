#include <gui/model/Model.hpp>
#include <gui/model/ModelListener.hpp>

Model::Model() : modelListener(0)
{
    for (int row = 0; row < row_size; ++row) {
        for (int column = 0; column < column_size; ++column) {
            matrix[row][column] = 0;  // 全ての要素を0に初期化
        }
    }
}

void Model::tick()
{

}
